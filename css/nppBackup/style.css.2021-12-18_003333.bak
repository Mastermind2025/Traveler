@font-face{
	font-family:HelveticaNeueCyr;
	src: url('../font/Roboto-Regular.ttf');
}

*{margin: 0; padding: 0; text-decoration: none;}
body{
	margin: 0;
	padding: 0 15px;
	background: gray;
	padding-top: 4px;
	box-sizing: border-box;
	font: 12pt Tahoma, sans-serif;
}
a{ text-decoration: none;} img{border: 0;} h1, h2, h3{ margin: 0;}
.flex{ display: flex; justify-content: space-between; flex-direction: row;}
header.header{
	display: none;
}
.text_small_02{ display: none;}
.text_small_01{ display: block;}
.avocado{
	/*border: 1px dashed red;*/
	height: 60px;
	margin: 110px 23px 0px;
	font-size: 14pt;
	font-weight: bolder;
	color: #25306c;
}
.section{
	display: flex;
	justify-content: space-between;
	width: 100%;
	background: maroon;}
.section_box{/* display: flex; */width: 100%;background: #c2d7ef;padding: 1px 1px;}
	
	.box{
	display: flex;
	justify-content: space-between;
	width: 100%;
	/* background: maroon; */
	padding: 353px 0 0;
	}
.logo_small{
display: block;
border: 1px dashed black;
padding: 0px 5px;
}
section.background{
	padding: 0 0 0;
	background-image: url('../img/back_plane01.png');
	background-repeat: no-repeat;
	background-position: left;
	background-size: cover;
	
	/* margin: 10px 0 0; */
}
section.second{/*second block*/
	background-image: url('../img/back_plane02.png');
	background-repeat: no-repeat;
	background-size: cover;
	background-position: left;
	display: flex;
	flex-direction: column;
	padding: 0px 0 40px;
}	
.bold_type{ font-weight: bold;}
.block_href{
	display: flex;
	justify-content: space-between;
}
.expedia_box{
	display: flex;
	justify-content: center;
	border: 1px dashed red;
}
.expedia{font-size: 48pt;padding: 3px;border: 1px solid red;}
.yapra_box{
	display: flex;
	flex-direction: column;
	border: 1px dashed olive;
	padding: 3px;
	font-size: 18pt;
	vertical-align: center;
}
.yapra_box_small{
	font-size: 23pt;
	border: 1px dashed orange;
	padding: 2px 0 0;
}

section.second h2
{background: #1c2556;/* width: 95%; */color: white;padding: 14px 19px;margin: 5px 0 40px 0;}
.airplaines_box
{display: flex;justify-content: space-between;flex-direction: row;
/* background: olive; */color: white;box-sizing: border-box;}
.airplaines_box h2{background: #1c2556;padding: 14px 21px;margin: 5px 0;
box-sizing: border-box;}
.back_cloud{
	padding: 0 0px;
}
