box-sizing: border-box; color: #fff;}

img.main_logo{
	display: block;
	border: 1px dashed red;
	padding: 5px 5px;
}
	
div.wrapper{
	margin: 0 auto;
	width: 87%;
	max-width: 1205px;
	/*height: 600px;*/
	/* background: silver; */
}
.journeys{
	color: white;
	font: 12pt Roboto-Regular, sans-serif;
	margin: 110px 0 0 0px;
	padding: 16px 33px 12px 138px;
	background: #041ca4;
	height: 28px;
	font-size: 16pt;
}
.green_box{
	width: 100%;
	border: 5px dashed white;
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
	flex-direction: row;
	box-sizing: border-box;
}
.green_box_l{flex: 0 1 calc(45% - 30px);padding: 19px 39px 17px 0;border: 10px dashed red;display: flex;/* justify-content: center; */flex-direction: column;/* background: green; */}
div.green_box_l_box:not(:last-child){/* background: silver; */ padding-right: 18px;}
div.green_box_l_box:nth-child(3){margin-top: 30px;/* background: yellow; */}	
	.green_box_l_box{
	border: 1px dashed silver;
	display: flex;
	flex-direction: row;
	justify-content: flex-end;
	}
	.green_box_l h3{ font-size: 22pt; }
	.green_box_l p{font-size: 18pt;line-height: 27px;}
	


.green_box_l a,
.green_box_l h3,
.green_box_l p{ border: 1px dashed yellow;}

.green_box_l_box h3{ padding: 0 15px;}

.green_box_right{ border: 1px dashed yellow; }

.green_box_right{
	display: flex;
	justify-content: flex-start;
	flex-direction: row;
	}
.green_box_complex	{ border: 3px dashed orange; }

.green_box_right_second{background: green;color: #fff;padding: 10px 11px;font-size: 16pt;}
.green_box_right_first{
    font-size: 18pt;
	font-weight: bold;
}
img.people{/* background: orange; */width: 100%;}
img.baloon{/* background: maroon; */width: 100%;}

h2.title_red{
    background: #8c0314;
    padding: 16px 33px 16px 67px;
    margin: 5px 0;
    box-sizing: border-box;
    color: #fff;
    font: 16pt Roboto-Regular, sans-serif;
}
div.box_with_finger{
	display: flex;
	justify-content: center;
	flex-wrap: wrap;
	margin-bottom: 26px;
}
.start_flex{ display: flex; justify-content: flex-start; flex-wrap: wrap;}
.start_center{ display: flex; justify-content: center; flex-wrap: wrap;}
.border_dash{ border: 1px dashed red; }

.title_h3 { padding: 0 15px; font-size: 22pt; }
.text_p { font-size: 18pt; line-height: 27px; }
.pad{padding-top: 19px;}
.background_light_blue{ background: #c2d7ef }
